<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="framework.modules.propertymanagement.accountreceivablemanage.domain.PhfeeInfo" table="tPhfeeInfo">
          <id name="pk" type="java.lang.String">
            <column name="PK" length="50" />
            <generator class="uuid" />
        </id>
        <property name="checkFlag" type="java.lang.String">
            <column name="CheckFlag" length="20"/>
        </property>
        <property name="clientCode" type="java.lang.String">
            <column name="ClientCode" length="50"/>
        </property>
        <property name="orgSysCode" type="java.lang.String">
            <column name="OrgSysCode" length="50"/>
        </property>
        <property name="receiveDate" type="java.lang.String">
            <column name="ReceiveDate" length="50" />
        </property>
        <property name="halthFee" type="java.lang.Double">
            <column name="HalthFee" precision="20" />
        </property>
        <property name="otherFee" type="java.lang.Double">
            <column name="OtherFee" precision="20" />
        </property>
        <property name="receiveRent" type="java.lang.Double">
            <column name="ReceiveRent" precision="20" />
        </property>
        <property name="realRecRent" type="java.lang.Double">
            <column name="RealRecRent" precision="20" />
        </property>
        <property name="payType" type="java.lang.String">
            <column name="PayType" length="20" />
        </property>
        <property name="payPerson" type="java.lang.String">
            <column name="PayPerson" length="50" />
        </property>
        <property name="invoice" type="java.lang.String">
            <column name="Invoice"  length="20"  />
        </property>
        <property name="invoiceCode" type="java.lang.String">
            <column name="InvoiceCode" length="50" />
        </property>
        <property name="lister" type="java.lang.String">
            <column name="Lister" length="50" />
        </property>
        <property name="listDate" type="java.lang.String">
            <column name="ListDate" length="50" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="Remark" length="500" />
        </property>
        <property name="inserttime" type="java.lang.String">
            <column name="Inserttime" length="40" />
        </property>
        <property name="lastestUpdate" type="java.lang.String">
            <column name="LastestUpdate"  length="40"/>
        </property>
        <property name="updatePerson" type="java.lang.String">
            <column name="UpdatePerson" length="40"/>
        </property>
    </class>
</hibernate-mapping>
